{
  "name": "quantum-ml-simulation",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "d3": "^7.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^0.0.0",
    "three": "^0.158.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "react-scripts start",
    "start:backend": "cd backend && ./venv/bin/uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start:backend:install": "cd backend && (./venv/bin/pip install -r requirements.txt || echo 'Dependencies already installed') && ./venv/bin/uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "setup": "npm install && cd backend && python3 -m venv venv && ./venv/bin/pip install -r requirements.txt",
    "dev": "npm run start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "postcss": "^8.4.16",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.1.8"
  }
}
